        "JetStream": {
            "Enable": true,
            "StreamName": "mystream",
            "Subjects": [ "events.*" ],
            "Storage": "File",
            "Retention": "Limits",
            "MaxMessages": 1000,
            "MaxBytes": 10485760,
            "MaxAge": "00:30:00",
            "MaxConsumers": -1,
            "Discard": "Old",
            "Replicas": 1,
            "DuplicatesWindow": "00:02:00",
            "AllowDirect": false,
            "AllowRollup": false
           
        }
    }
}
